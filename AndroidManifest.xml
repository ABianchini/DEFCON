<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.advancementbureau.defconwork"
    android:versionCode="15"
    android:versionName="1.3.1" >

    <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:icon="@drawable/defcon_icon"
        android:label="@string/app_name">
        <activity android:name="com.advancementbureau.defcon.DefconSplashActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.advancementbureau.defcon.DefconActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name="com.advancementbureau.defcon.DefconSettingsActivity" android:icon="@drawable/settings" android:label="@string/settings"></activity>
        <activity android:name="com.advancementbureau.defcon.DefconChangelogActivity" android:icon="@drawable/time" android:label="@string/changelog"></activity>
        <activity android:name="com.advancementbureau.defcon.SuperDefconActivity" android:label="@string/app_name"></activity>
        <activity android:name="com.advancementbureau.defcon.DefconLogActivity" android:label="@string/log" android:icon="@drawable/list"></activity>
    </application>

</manifest>